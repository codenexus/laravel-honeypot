{
    "name": "codenexus/laravel-honeypot",
    "description": "Honeypot spam prevention.",
    "license": "MIT",
    "keywords": ["laravel", "spam", "forms", "honeypot"],
    "authors": [
        {
            "name": "Jeremy Schaffer",
            "email": "jeremys@codenexus.org"
        }
    ],
    "require": {
        "php": "^7.0",
        "laravel/framework": "~5.4.0|~5.5.0|~5.6.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.3|^7.0"
    },
    "autoload": {
        "psr-4": {
            "Codenexus\\Honeypot\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Codenexus\\Honeypot\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Codenexus\\Honeypot\\HoneypotServiceProvider"
            ],
            "aliases": {
                "Honeypot": "Codenexus\\Honeypot\\Facades\\Honeypot"
            }
        }
    }
}